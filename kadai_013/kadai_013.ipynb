      "source": [
        "def calculation_consumption_tax(amount, tax_rate):\n",
        "    #かけ率を出す\n",
        "    rate = tax_rate / 100\n",
        "    #消費税分の計算\n",
        "    consumption_tax = amount * rate\n",
        "    return consumption_tax + amount"
      ]
    },
    {
